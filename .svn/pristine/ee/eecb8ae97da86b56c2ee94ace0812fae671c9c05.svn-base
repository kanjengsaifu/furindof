          foreach ($this->selectQuery->result_array() as $row) 
          {
            $strComboAjax.= "<option value='".$row['IDOutput']."'>".$row['NamaOutput']."</option>";
          }

          $strComboAjax .= "</select></div></div>";
        }

        return $strComboAjax;
      }

